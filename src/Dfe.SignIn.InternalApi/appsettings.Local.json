  "Platform": {
    "SurveyUrl": "https://survey.localhost",
    "ServicesUrl": "https://services.localhost"
  },
  "SecurityHeaderPolicy": {
    "AllowedOrigins": [
      "*.signin.education.gov.uk",
      "localhost:*"
    ]
  },
  "BlockedEmailAddresses": {
    "BlockedDomains": [
      "blocked.com"
    ],
    "BlockedNames": [
      "admin"
    ]
  },
  "EntityFramework": {
    "Directories": {
      "ConnectionString": "<connection_string>"
    },
    "Organisations": {
      "ConnectionString": "<connection_string>"
    }
  }
