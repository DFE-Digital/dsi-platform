@if (ViewData[DsiLayoutConstants.Crumbs] is IEnumerable<CrumbViewModel> crumbs && crumbs.Any())
{
    <govuk-breadcrumbs>
        @foreach (var crumb in crumbs)
        {
            <govuk-breadcrumbs-item href="@crumb.Href">@crumb.Text</govuk-breadcrumbs-item>
        }
    </govuk-breadcrumbs>
}
else if (ViewData[DsiLayoutConstants.BackLink] is BackLinkViewModel backLink)
{
    <govuk-back-link href="@backLink.Href">@backLink.Text</govuk-back-link>
}
