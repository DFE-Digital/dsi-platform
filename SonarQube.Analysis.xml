<SonarQubeAnalysisProperties  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
	<!-- optional properties -->
	<Property Name="sonar.host.url">https://sonarcloud.io</Property>

	<!-- analysis exclusions -->
	<Property Name="sonar.exclusions">docs/templates/dsi/**,**/snapshots/**,frontend/**,**/Configuration/**/*Extensions.cs</Property>

	<!-- coverage reporting -->
	<Property Name="sonar.coverageReportPaths">CoverageReport/SonarQube.xml</Property>
	<Property Name="sonar.coverage.exclusions">**/Program.cs,**/*Options.cs,**/*Constants.cs,**/*.cshtml,**/*.js,**/Fake**</Property>
	<Property Name="sonar.cs.vstest.reportsPaths">**/*.trx</Property>
</SonarQubeAnalysisProperties>
